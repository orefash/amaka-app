<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
<head>
  
    <title>Add Complaint</title>
    
  
    <% include /partials/head %>
    <!-- <script type="text/javascript" src="https://cdn.jsdelivr.net/html5shiv/3.7.3/html5shiv.min.js"></script> -->

</head>
<body>
    <!-- Left Panel -->

    
  <% include /partials/left %>
    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
      <% include /partials/header %>  
      <!-- Header-->

        <div class="breadcrumbs">
            <div class="breadcrumbs-inner">
                <div class="row m-0">
                    <div class="col-sm-4">
                        <div class="page-header float-left">
                            <div class="page-title">
                                <h1>Student's Complaint</h1>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-8">
                        <div class="page-header float-right">
                            <div class="page-title">
                                <ol class="breadcrumb text-right">
                                    <li><a href="#">Student Complaints</a></li>
                                    <li class="active">New Complaint</li>
                                </ol>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="content">
            <div class="animated fadeIn">


                <div class="row">

                    <div class="col-3"></div>
                   
                    <div class="col-6 col-offset-3">
                        <div class="card">
                            <div class="card-header"><strong>New Complaint</strong></div>
                            <div class="card-body card-block">

                              <form method="POST" action="/new-complaint">
                                
                                <div class="form-group"><label for="Sid" class=" form-control-label">Admission No.</label>
                                    <input disabled="" name="sid" type="text" value="<%= sid %>" id="sid" placeholder="Enter Student's Admission No." class="form-control">
                              </div>
                              
                              <input hidden  name="s_id" type="text" value="<%= sid %>" id="sid" >
                             
                                
                                <div class="form-group"><label for="fname" class=" form-control-label">First Name</label>
                                    <input disabled="" value="<%= fname %>" name="fname" type="text" id="fname" placeholder="Enter Student's First Name" class="form-control"></div>
                                
                                <div class="form-group"><label for="lname" class=" form-control-label">Last Name</label>
                                    <input disabled="" value="<%= lname %>" name="lname" type="text" id="lname" placeholder="Enter Student's Last Name" class="form-control"></div>
                              
                               
                                <div class="form-group"><label for="complaint" class=" form-control-label">Complaint</label>
                                    
                                    <textarea name="complaint" id="complaint" rows="6" placeholder="Enter Student's Complaint" class="form-control"></textarea>
                                </div>
                                
                                
                                <div class="form-group"><label for="feedback" class=" form-control-label">Nurse Feedback</label>
                                    <textarea name="feedback" id="feedback" rows="6" placeholder="Enter Feedback" class="form-control"></textarea>
                                </div>
                                
                                
                                
                                <div class="form-group"><label for="treatment" class=" form-control-label">Treatment Administered</label>
                                    <textarea name="treatment" id="treatment" rows="6" placeholder="Enter Treatment Administered" class="form-control"></textarea>
                                </div>
                                
                                
                                
                                <div class="form-actions form-group"><button type="submit" class="btn btn-success btn-sm">Submit</button></div>

                                
                              </form>  
                              
                            </div>
                        </div>
                    </div>

                    
                    

            </div>


        </div><!-- .animated -->
    </div><!-- .content -->

    <div class="clearfix"></div>

    <% include /partials/footer %>


</body>
</html>
